package com.project.service;

import com.project.dao.RegisterDao;
import com.project.dao.RegisterDaoImpl;
import com.project.entity.Customer;
import com.project.exception.SomethingWentWrongException;

public class RegisterCusImpl implements RegisterCus{

	@Override
	public void registerCustomer(Customer customer) throws SomethingWentWrongException {
		RegisterDao registerDao =  new RegisterDaoImpl();
		registerDao.registerCus(customer);
		
	}

}
